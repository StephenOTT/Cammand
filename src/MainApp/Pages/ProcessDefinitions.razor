@using MainApp.Components.ProcessDefinition
@using Camunda.Http.Api

@inject AppData AppData

@page "/process-definitions"

<MudContainer MaxWidth="MaxWidth.ExtraLarge" Class="mt-16">
    <MudText Typo="Typo.h3" GutterBottom="true">Process Definitions</MudText>
    <ProcessDefinitionDataGrid Camunda="@_camunda" />
</MudContainer>

@code {
    CamundaClient _camunda;

    protected override void OnInitialized()
    {
        _camunda = AppData.CamundaClient;
    }
}
